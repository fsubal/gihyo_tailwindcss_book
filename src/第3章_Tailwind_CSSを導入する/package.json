{
  "name": "chapter3",
  "version": "0.0.0",
  "description": "第3章 Tailwind CSSを導入する",
  "scripts": {
    "build": "run-s --print-label build:*",
    "build:tailwind-cli": "tailwindcss --config tailwind.config.js --input src/main.css --output dist/main.css",
    "build:postcss-cli": "postcss src/main.css --output dist/main.css",
    "build:webpack": "webpack build --progress",
    "build:gulp": "gulp tailwindcss:build"
  },
  "devDependencies": {
    "@babel/core": "^7.28.6",
    "@babel/preset-env": "^7.29.0",
    "autoprefixer": "^10.4.24",
    "css-loader": "^7.1.3",
    "gulp": "^5.0.1",
    "gulp-postcss": "^10.0.0",
    "mini-css-extract-plugin": "^2.10.0",
    "postcss": "^8.5.6",
    "postcss-cli": "^11.0.1",
    "postcss-loader": "^8.2.0",
    "webpack": "^5.104.1",
    "webpack-cli": "^6.0.1"
  }
}
